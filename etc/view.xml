    <vars module="Magento_Catalog">
        <var name="gallery">
            <var name="allowfullscreen">true</var>
            <var name="keyboard">true</var>
            <var name="loop">true</var>
            <var name="nav">true</var> <!-- true/false -->
            <var name="navdir">horizontal</var> <!-- horizontal/vertical -->
        </var>
    </vars>
    <vars module="Swissup_Breeze">
        <var name="enabled">1</var>
        <var name="turbo">1</var>
        <var name="sizes">
            <var name="category_page_grid">(min-width: 1536px) 300px, (min-width: 1024px) 230px, (min-width: 768px) 165px, (min-width: 640px) 190px, calc(50vw - 2rem)</var>
            <var name="category_page_grid-1column">(min-width: 1536px) 285px, (min-width: 1280px) 235px, (min-width: 1024px) 300px, (min-width: 768px) 230px, (min-width: 640px) 190px, calc(50vw - 2rem)</var>
            <var name="category_page_grid-3columns">(min-width: 1536px) 235px, (min-width: 1280px) 170px, (min-width: 1024px) 150px, (min-width: 768px) 165px, (min-width: 640px) 190px, calc(50vw - 2rem)</var>
            <var name="category_page_list">(min-width: 1280px) 300px, (min-width: 1024px) 260px, (min-width: 768px) 190px, 35vw</var>
            <var name="product_page_image_medium">(min-width: 1536px) 700px, (min-width: 1280px) 615px, (min-width: 1024px) 490px, (min-width: 768px) 360px, (min-width: 640px) 625px, calc(100vw - 4rem)</var>
